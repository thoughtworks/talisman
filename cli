cyan_bold_ln() { println_in_color "\e[1;36m" "$*"; }
white_bold_ln() { println_in_color "\e[1;37m" "$*"; }
red_underlined_ln() { println_in_color "\e[4;31m" "$*"; }
green_underlined_ln() { println_in_color "\e[4;32m" "$*"; }
